{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "App for those who forget things",
  "scripts": {
    "sync-frontend": "webpack -d -w",
    "start-dev-server": "./node_modules/.bin/nodemon server/server.js --exec ./node_modules/.bin/babel-node",
    "start": "./node_modules/.bin/concurrently 'npm run sync-frontend' 'npm run start-dev-server'",
    "prod": "./node_modules/.bin/webpack -p && ./node_modules/.bin/babel server -d dist && node dist/server.js",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register -R spec tests"
  },
  "author": "z.gatelis",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lodash": "^4.15.0",
    "material-ui": "^0.15.4",
    "moment": "^2.14.1",
    "mongodb": "^2.2.9",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-root-import": "^4.1.1",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  }
}
